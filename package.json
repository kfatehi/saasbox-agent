  "description": "Extends the ydm api to include reverse proxying",
  "main": "src/server/app.js",
  "scripts": {
    "test": "mocha --recursive test",
    "tdd": "watchy -w . -i '/\\.|node_modules|\\.json|data|coverage$' -- mocha -R min --recursive test 2>&1 | simple-stacktrace",
    "autocover": "watchy -w . -i '/\\.|node_modules|\\.json|data|coverage$' -- istanbul cover _mocha -- --recursive test",
    "cover": "istanbul cover _mocha -- --recursive test"
  },
  "repository": {
    "type": "git",
