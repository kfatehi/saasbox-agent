  },
  "author": "Keyvan Fatehi",
  "license": "",
  "private": true,
  "dependencies": {
    "body-parser": "^1.6.7",
    "express": "^4.8.7",
    "http-proxy": "^1.3.0",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "node-localstorage": "^0.3.4",
    "winston": "^0.7.3",
    "ydm": "^1.0.9",
    "uri-js": "~1.4.2"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.21.4",
    "supertest": "^0.13.0"
  }
